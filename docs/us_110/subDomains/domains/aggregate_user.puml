@startuml
package "Aggregate User" {

    ' Agregado User (Aggregate Root)
    class User <<AggregateRoot>> {
        - id: UserId
        - username: Username
        - password: Password
        - email: String
        - phone: String
    }

    ' Value Objects associados
    class UserId <<ValueObject>> {
        - value: String
    }

    class Username <<ValueObject>> {
        - value: String
    }

    class Password <<ValueObject>> {
        - value: String
    }

    ' Ligações
    User --> UserId
    User --> Username
    User --> Password
}
@enduml