@startuml
hide methods

package "Aggregate Show Request " {

    ' Agregado ShowRequest (Aggregate Root)
    class ShowRequest <<AggregateRoot>> {
        ShowRequestId: Id
        Client: Client
        Description: String
    }

    ' Value Objects associados
    class Id <<(V, #007fff) ValueObject>> {
        value: String
    }

    ' Ligações
    ShowRequest --> Id
}
@enduml